{
    "module":  "./dist/index.js",
    "peerDependencies":  {
                             "react":  "\u003e=18",
                             "react-dom":  "\u003e=18"
                         },
    "type":  "module",
    "types":  "./dist/index.d.ts",
    "version":  "0.1.0",
    "files":  [
                  "dist",
                  "tokens"
              ],
    "scripts":  {
                    "build":  "tsc -p tsconfig.json"
                },
    "name":  "@my-forum/ui",
    "private":  false,
    "exports":  {
                    "./tokens":  "./tokens/tokens.json",
